package com.hcl.inheritance;

class c1 {
	public void show() {
		 System.out.println("From show method");
	}
}
class c2 extends c1 {
	public void display() {
		System.out.println("From display method");
	}
}

public class InhDemo {
	public static void main(String[] args) {
		c2 obj=new c2();
		obj.show();
		obj.display();
	}

}

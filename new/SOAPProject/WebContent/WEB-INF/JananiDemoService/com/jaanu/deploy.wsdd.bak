<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from JananiDemoService WSDL service -->

  <service name="JananiDemo" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://jaanu.com"/>
      <parameter name="wsdlServiceElement" value="JananiDemoService"/>
      <parameter name="schemaQualified" value="http://jaanu.com"/>
      <parameter name="wsdlServicePort" value="JananiDemo"/>
      <parameter name="className" value="com.jaanu.JananiDemoSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="JananiDemo"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getInt" qname="operNS:getInt" xmlns:operNS="http://jaanu.com" returnQName="retNS:getIntReturn" xmlns:retNS="http://jaanu.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="getInt"/>

  </service>
</deployment>

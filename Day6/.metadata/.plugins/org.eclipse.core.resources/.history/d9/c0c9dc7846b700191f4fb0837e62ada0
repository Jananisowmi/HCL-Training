package com.hcl.collec;

interface IHello{
	void sayHello();
}

public class Lamda2 {
public static void main(String[] args) {
	IHello h1 = ()-> {
		System.out.println("Hello from janu...");
	};
	
	IHello h2 = () -> {
		System.out.println("hello from virat..");
	};
	
	IHello h3 = () -> {
		System.out.println("Hello from anisha..");
	};
	
	h1.sayHello();
	h2.sayHello();
	h3.sayHello();
}
}
